[user]
	email = YOUR_EMAIL
	name = YOUR_NAME
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	lg = log --color --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	lo = log --pretty=oneline
	df = "!f() { git diff --name-status; echo \"---\"; git diff --name-status --cached; }; f"
[color]
	ui = true
[core]
	editor = vim

